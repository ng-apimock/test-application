    {
      "type": "MIT",
      "url": "https://github.com/ng-apimock/test-application/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "apimock",
    "ng-apimock",
    "test-application",
    "http",
    "api",
    "scenario",
    "mocking",
    "endpoints",
    "responses",
    "delay",
    "recording"
  ],
  "devDependencies": {
    "@angular/animations": "15.0.3",
    "@angular/cdk": "15.0.2",
    "@angular/common": "15.0.3",
