    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "15.0.3",
    "@angular/compiler-cli": "15.0.3",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@ng-apimock/core": "3.6.0",
    "@ng-apimock/dev-interface": "3.1.1",
