  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-apimock/test-application/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "apimock",
    "ng-apimock",
    "test-application",
    "http",
    "api",
    "scenario",
    "mocking",
    "endpoints",
    "responses",
    "delay",
    "recording"
  ],
  "devDependencies": {
    "@angular/animations": "14.0.6",
    "@angular/cdk": "14.0.4",
    "@angular/common": "14.0.6",
