    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "semantic-release": "19.0.5",
    "tslib": "2.4.1",
    "ts-node": "10.9.1",
