    "express": "4.18.1",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "semantic-release": "19.0.3",
    "tslib": "2.4.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
