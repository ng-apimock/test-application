    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "commitizen": "4.2.4",
    "eslint": "8.2.0",
    "express": "4.17.1",
    "http-proxy-middleware": "2.0.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "semantic-release": "18.0.0",
    "tslib": "2.3.1",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
